{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "noEmit": true // Ensure no files are emitted
  },
  "include": [
    "/**.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}